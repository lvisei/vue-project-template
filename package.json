{
  "name": "vue-project-template",
  "private": true,
  "version": "1.0.0",
  "description": "A vue project template by webpack",
  "author": "liuvigongzuoshi@foxmail.com",
  "scripts": {
    "dev": "npm run start",
    "start": "webpack-dev-server --config build/server",
    "build": "webpack --config build/build",
    "build:r": "npm run build --report",
    "lint": "eslint --ext .js,.vue src",
    "lint-fix": "eslint --fix --ext .js,.vue src",
    "prettier": "prettier -c --write 'src/**/*.{js,vue}'",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "vue": "^2.6.11",
    "vue-router": "^3.3.2",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.10.2",
    "@intervolga/optimize-cssnano-plugin": "^1.0.6",
    "autoprefixer": "^9.8.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.2.5",
    "less": "^3.11.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.2.9",
    "mini-css-extract-plugin": "^0.8.2",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "sass-loader": "^8.0.2",
    "url-loader": "^3.0.0",
    "vue-loader": "^15.9.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix"

    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,md,vue,css,less,scss}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 6.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
